@model IEnumerable<MVCBikeShop.Models.User>
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<h2>Users list</h2>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Access layer</a>
            </td>
        </tr>
    }
</table>